    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script>
        $(function(){
            
            var prog = 1;
            var index;
            var e = 0, s = 0, t = 0, j = 0;
            var mbti = "";
            $(".q button").on("click",function(){
                index = $(this).parent("li").index();
                if($(this).hasClass("e")){
                    e++;
                } else if($(this).hasClass("s")){
                    s++;
                } else if($(this).hasClass("t")){
                    t++;
                } else if($(this).hasClass("j")){
                    j++;
                }
                if(prog<=12){
                    $(".q li").removeClass("now");
                    $("li").eq(index).addClass("hidden");
                    $("li").eq(index+1).addClass("now");
                    $(".bar").attr('style','width: calc(100/12*'+prog+'%)');
                    $(".progressNum").children("span").text(prog);
                    prog++;
                } else {
                    $(".q").fadeOut(1000);                    
                    $(".progress").fadeOut(1000);
                    $(".progressNum").fadeOut(1000);
                    $(".n").fadeIn(1000);
                }
                console.log(e, s, t, j);
            });
            $(".n button").on("click",function(){
                (e>=2)?mbti+="e":mbti+="i";
                (s>=2)?mbti+="s":mbti+="n";
                (t>=2)?mbti+="t":mbti+="f";
                (j>=2)?mbti+="j":mbti+="p";
                console.log(mbti);
                $(".n").fadeOut(1000);
                $(".a").fadeIn(1000);
                $("."+mbti).addClass("now");
                var height = $("."+mbti).outerHeight();
                $(".a").height(height + 80);
            });

        });
    </script>
